{
  "name": "ra-media-library",
  "version": "1.0.4",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "keywords": [
    "react-admin",
    "media-library",
    "react",
    "mui",
    "file-manager",
    "admin-panel",
    "react-component",
    "ra-component",
    "ra-media-library"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "babel src --out-dir dist/cjs --env-name cjs --copy-files",
    "build:esm": "babel src --out-dir dist/esm --env-name esm --copy-files"
  },
  "peerDependencies": {
    "react": ">=17.0.0 <20.0.0",
    "react-dom": ">=17.0.0 <20.0.0",
    "react-admin": ">=4.0.0 <6.0.0",
    "@mui/material": ">=5.0.0 <8.0.0"
  },
  "devDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-admin": "^5.12.3",
    "@mui/material": "^7.3.5",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0"
  }
}
